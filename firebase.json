{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
			{
				"source": "/getRandomWord",
				"function": "getRandomWord"
			},
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
